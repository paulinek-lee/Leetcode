import math

class Solution(object):
    def constructRectangle(self, area):
        """
        :type area: int
        :rtype: List[int]
        """
        
        width = math.sqrt(area)
        width = math.floor(width)
         
        while area%width!=0:
            width= width-1
        
        return([int(area/width), int(width)])

    
            
        
