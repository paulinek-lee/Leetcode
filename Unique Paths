class Solution:
    def uniquePaths(self, m: int, n: int) -> int:
        total = m+n-2

        big = max(m,n) - 1
        small = min(m,n) - 1
        
        dividend = 1
        
        for i in range(big+1,total+1):
            dividend*=i
        
        divisor = 1
        
        for j in range(1,small+1):
            divisor*=j
        
        return(dividend/divisor)
