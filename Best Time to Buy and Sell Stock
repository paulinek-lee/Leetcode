class Solution(object):
    def maxProfit(self, prices):
        """
        :type prices: List[int]
        :rtype: int
        """
        max_diff = 0
        lowest = prices[0]

        for i in range(1, len(prices)):
            current = prices[i]
            
            if lowest>current:
                lowest = current
                
            if lowest <= current:
                if current-lowest>max_diff:
                    max_diff = current-lowest
        
        return(max_diff)
                
